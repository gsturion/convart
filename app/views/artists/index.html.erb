<div id="artists">
  <h1>Artists</h1>
  <%= link_to "Voltar", events_path%>
  <p><%= @event.edition %></p>
  <ul>
    <% @artists.each do |artist| %>
      <li>
        <span> <a href="artists/1"> <%= artist.name %> </a> </span>
        <span><%= artist.nationality %></span>
        <span><%= artist.delegation %></span>
      </li>
    <% end %>
  </ul>
</div>
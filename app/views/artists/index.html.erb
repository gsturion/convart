<div id="artists">
<%= link_to "Voltar", events_path, class: "back-link"%>
<br>
<div class="row justify-content-center ">
  <h3>Artistas</h3>
</div>

<p><%= @event.edition %></p>

<table>
  <tbody>
    <% @artists.each do |artist| %>
      <tr>
        <td><%= link_to artist.name, event_artist_path(@event,artist) %></td>
        <td><%= artist.nationality %></td>
        <td><%= artist.delegation %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
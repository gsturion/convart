<%= link_to "Back", pages_map_path, class: "back-link"%>
<div class="row justify-content-center ">
  <h3>Events</h3>
</div>
<div class="row justify-content-center ">
<table>
  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= link_to event.edition, event_path(event) %></td>
        <td><%= event.place %></td>
        <td><%= event.inicial_date %></td>
        <td><%= event.final_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>